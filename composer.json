{
    "name": "atantares/temporal-bundle",
    "description": "Workflow Runtime in Symfony for Temporal",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "atantares",
            "email": "vod.ukr@gmail.com"
        }
    ],
    "keywords": [
        "temporal",
        "symfony",
        "bundle"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "symfony/dependency-injection": "^6.0 || ^7.0",
        "temporal/sdk": "^2.7",
        "symfony/http-kernel": "^6.0 || ^7.0",
        "symfony/config": "^6.0 || ^7.0",
        "symfony/console": "^6.0 || ^7.0",
        "symfony/serializer": "^6.0 || ^7.0",
        "symfony/property-info": "^6.0 || ^7.0",
        "symfony/runtime": "^6.0 || ^7.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.15",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.22",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-symfony": "^1.3",
        "psr/log": "^3.0",
        "sentry/sentry-symfony": "^4.10",
        "nyholm/symfony-bundle-test": "^3.0",
        "phpunit/phpunit": "^10.3",
        "symfony/monolog-bundle": "^3.8",
        "symfony/web-profiler-bundle": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Atantares\\TemporalBundle\\": "src/"
        },
        "files": [
            "src/DependencyInjection/function.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Atantares\\TemporalBundle\\Test\\": "tests"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/runtime": true
        }
    }
}

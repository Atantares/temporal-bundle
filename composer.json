{
    "name": "atantares/temporal-bundle",
    "description": "Command Runner and Activity Registry for Temporal. Fork from highcoreorg/temporal-bundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "loper",
            "email": "bizrenay@gmail.com"
        },
        {
            "name": "atantares",
            "email": "vod.ukr@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
        "temporal/sdk": "^2.5",
        "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
        "symfony/config": "^5.4 || ^6.0 || ^7.0",
        "symfony/console": "^5.4 || ^6.0 || ^7.0",
        "symfony/serializer": "^5.4 || ^6.0 || ^7.0",
        "symfony/property-info": "^5.4 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Atantares\\TemporalBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Atantares\\TemporalBundle\\": "tests"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
